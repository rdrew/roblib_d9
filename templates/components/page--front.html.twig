<div class="layout-container">

<!--___________________Topnav___________________-->

<div class="topnav__wrapper">
  <div class="container topnav__container">
        {% include '@roblib_d9/components/topnav/topnav--mobile.html.twig'  %}
        {% include '@roblib_d9/components/topnav/topnav.html.twig'  %}
  </div>
</div>
  <header role="banner">
    <div class="container">
    {{ page.header }}
    </div>
  </header>

  {{ page.primary_menu }}
  {{ page.secondary_menu }}

  {{ page.breadcrumb }}

  {{ page.highlighted }}

  {{ page.help }}

  <main role="main">
    <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}

    <div class="layout-content">
      {{ page.content }}
    </div>{# /.layout-content #}

  </main>

  {% if page.footer %}
    <footer role="contentinfo">
      {{ page.footer }}
      {{ page.subfooter }}
    </footer>
    
  {% endif %}

</div>{# /.layout-container #}
